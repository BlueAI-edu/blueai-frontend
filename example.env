# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values
# Note: All variables must start with REACT_APP_ to be accessible in the frontend

# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:8000

# Azure AD Configuration (for Microsoft authentication)
REACT_APP_AZURE_CLIENT_ID=your-azure-frontend-client-id
REACT_APP_AZURE_BACKEND_CLIENT_ID=your-azure-backend-client-id

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-oauth-client-id

# Health Check Plugin (optional)
# Set to 'true' to enable health monitoring during development
ENABLE_HEALTH_CHECK=false

# Note: NODE_ENV is automatically set by React scripts
# - 'development' when running npm start
# - 'production' when running npm run build
